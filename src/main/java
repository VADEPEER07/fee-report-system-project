import java.sql.*;

public class Main {
    public static void main(String[] args) {
        try {
            Connection conn = DriverManager.getConnection("jdbc:mysql://localhost/FeeReport", "root", "");
            Statement stmt = conn.createStatement();
            ResultSet rs = stmt.executeQuery("SELECT * FROM Students");
            while (rs.next()) {
                System.out.println("Student: " + rs.getString("name") + ", Paid: â‚¹" + rs.getDouble("fee_paid"));
            }
            conn.close();
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
